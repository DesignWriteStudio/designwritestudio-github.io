                      watchedDeals         : [

],
                      remindersWeblab      : "T1",
                      hasEverWatchedDeal   : null
                });
            };

            var params = {
                success: function(result) {
                    notifierResourcesCallback(result);
                },
                url: '/gp/deal/ajax/getNotifierResources.html',
                type: "POST",
                data: {},
                dataType: 'json'
            };

            A.$.ajax(params);

