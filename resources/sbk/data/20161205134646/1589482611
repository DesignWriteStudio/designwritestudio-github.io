<script type="text/javascript">
    P.when("A", "jQuery").execute(function(A, $) {
        A.on('rentalsFTRequest', function(rentalsRequestObj) {
            
            if (!rentalsRequestObj || !rentalsRequestObj.quantity || !rentalsRequestObj.asin || !rentalsRequestObj.merchantId || !rentalsRequestObj.offerContext) {
                return;
            }

            $.ajax({
                type: "POST",
                url: "/gp/product/features/dp-fast-track/udp-ajax-handler/get-quantity-update-message.html?ie=UTF8",
                contentType: 'application/x-www-form-urlencoded;charset=utf-8',
                data: rentalsRequestObj,
                dataType: "html",
                success: function(objResponse) {
                    A.trigger('rentalsFTResponse', objResponse);
                }
            });
        });
    });
</script>

    


