<script type="text/javascript">


window.registeredLocationUXPrePostRefreshHandler || P.when("A").execute(function(A) {
    A.on("UnifiedLocationPreRefresh", window.BXPrimeSwatchFuncs.disableOneClickButton);
    A.on("UnifiedLocationPostRefresh", function(success) {
        if (typeof success !== 'undefined' && success === false) {
            A.$("#getItBy").html("");
        }
        window.BXPrimeSwatchFuncs.enableOneClickButton();
    });
    window.registeredLocationUXPrePostRefreshHandler = true;
});

P.when('A').execute(function(A) {
    var updatePrimeFunction = function(data) {
        var form = A.$("#addToCart");
        var urlParams = window.BXPrimeSwatchFuncs.getUrlParams();
        var data = {
            'dropdown-selection': A.$("#unifiedLocation1ClickAddress").attr("value"),
            a : form.find("input[name=ASIN]").attr("value"),
            me: form.find("input[name=merchantID]").attr("value"),
            olid: form.find("input[name=offerListingID]").attr("value"),
            wdg: 'book_display_on_website',
            horizonte: '1',
            quantity: A.$("#quantity").attr("value"),
        };
        data = A.extend(urlParams, data);

        var ajaxUpdate = A.get('/gp/product/du/prime-ajax-aui-update.html',
            { params: data });
    };
    A.on("a:dropdown:quantity:select", updatePrimeFunction);
    
    
    A.on("ready", function(data) {
    
        
        
            A.$("#getItBy").css("display","block");
        
    
    });
});

if (typeof (window.BXPrimeSwatchFuncs) === 'undefined') {
    window.BXPrimeSwatchFuncs = new Object();
}

window.BXPrimeSwatchFuncs.updateOneClickSwatchSelection = function (count){
    var currentSwatch = jQuery('#currentSwatchSelection');
    var prevSelectedSwatchCount = currentSwatch.attr('value');
    if (prevSelectedSwatchCount != count) {
        currentSwatch.attr('value',count);

        //  Change CSS for the newly selected swatch
        jQuery('#bb_swatch_'+count).removeClass("oneClickSwatchAvailable").removeClass("oneClickSwatchHover").addClass("oneClickSwatchSelect");
        jQuery('#bb_ft_'+count).removeClass("oneClickFastTrackHover").addClass("oneClickFastTrackSelect").show();

        //  Change CSS for the swatch that is no longer selected
        jQuery('#bb_swatch_'+prevSelectedSwatchCount).removeClass("oneClickSwatchSelect").addClass("oneClickSwatchAvailable");
        jQuery('#bb_ft_'+prevSelectedSwatchCount).hide();

        window.BXPrimeSwatchFuncs.updateOneClickButton(count);
    }
}

window.BXPrimeSwatchFuncs.showOneClickSwatchHover = function (count) {
    var currentSwatchCount = jQuery('#currentSwatchSelection').attr('value');
    if (currentSwatchCount != count) {
        jQuery('#bb_ft_'+currentSwatchCount).hide();

        jQuery('#bb_swatch_'+count).removeClass("oneClickSwatchAvailable").addClass("oneClickSwatchHover");
        jQuery('#bb_ft_'+count).removeClass("oneClickFastTrackSelect").addClass("oneClickFastTrackHover").show();
    }
}

window.BXPrimeSwatchFuncs.removeOneClickSwatchHover = function (count) {
    var currentSwatchCount = jQuery('#currentSwatchSelection').attr('value');
    if (currentSwatchCount != count) {
        jQuery('#bb_swatch_'+count).removeClass("oneClickSwatchHover").addClass("oneClickSwatchAvailable");
        jQuery('#bb_ft_'+count).removeClass("oneClickFastTrackHover").addClass("oneClickFastTrackSelect").hide();

        jQuery('#bb_ft_'+currentSwatchCount).show();
    }
}

window.BXPrimeSwatchFuncs.updateOneClickButton = function (count) {
    // Trigger AUI event to allow the page to react to the change of swatch.
    P.when('A').execute(function(A) { A.trigger('updateOneClickButtonEvent', count); });

    // Do the UI update for the swatches.
    if (jQuery('#getItBy .oneClickSwatchOuter').length == 0) {
        jQuery('#oneClickBuyButton').attr('name', jQuery('#getItBy input').val());
    } else if (typeof count != 'undefined') {
        jQuery('#oneClickBuyButton').attr('name', jQuery('#bb_swatch_' + count + ' input').val());
    }
}

window.BXPrimeSwatchFuncs.disableOneClickButton = function () {
    jQuery('#oneClickAvailable').css('filter', 'alpha(opacity=50)').css('opacity', '0.5');
    jQuery('#oneClickBuyButton').attr('disabled', 'disabled').css('cursor', 'default');
}

window.BXPrimeSwatchFuncs.enableOneClickButton = function () {
    jQuery('#oneClickAvailable').css('filter', 'alpha(opacity=100)').css('opacity', '1');
    jQuery('#oneClickBuyButton').removeAttr('disabled').css('cursor', 'pointer');
}

//Because the 'asin' field on the page is defined as the parent asin for twister
//detail pages, use this function to retrieve the presently displayed (aka child) asin.
//Note that this function will return null if the DetailPage object isn't defined.
window.BXPrimeSwatchFuncs.retrieveDisplayedASINFromDetailPage = function() {
	if (typeof DetailPage != "undefined" && typeof DetailPage.StateController != "undefined") {
     var dpState = DetailPage.StateController.getState();
     return dpState["current_asin"];
 }
 return null;
}




window.BXPrimeSwatchFuncs.refreshSwatches = function () {
	location.reload(true);
}




window.BXPrimeSwatchFuncs.getUrlParams = function() {
    var urlParams = {}, keyValue;
    var excludedRefreshParams = {
        "Tag":1,
        "tag":1,
        "tag_value":1,
        "tag-value":1,
        "AssociateTag":1
    };
    var qIndex = window.location.href.indexOf('?');
    if (qIndex > 0) {
        var entries = window.location.href.slice(qIndex + 1).split('&');
        for (var i = 0; i < entries.length; i++) {
            if (entries[i]) {
                keyValue = entries[i].split('=');
                var key = keyValue.shift();

                // Determine if this query parameter should be exluded from a refresh
                if (!(key in excludedRefreshParams)) {
                    urlParams[decodeURI(key)] = decodeURI(keyValue.join('='));
                }
            }
        }
    }
    return urlParams;
}

window.BXPrimeSwatchFuncs.refreshSwatchesForQuantity = function (asin, me, onPageLoad) {
    window.BXPrimeSwatchFuncs.disableOneClickButton();

    // Grab the url parameters
    var urlParams = window.BXPrimeSwatchFuncs.getUrlParams();

    // If no ASIN is present, retrieve it.
    if (!asin) {
        asin = window.BXPrimeSwatchFuncs.retrieveDisplayedASINFromDetailPage();
    }

    // Retrieve the encrypted address (from 1-click dropdown or LocationUX).
    var dropdownSelection = jQuery('#buyboxAddressSelectionDropDown').attr('value');
    if (!dropdownSelection) {
        dropdownSelection = jQuery('#unifiedLocation1ClickAddress').attr('value');
    }

    // Get the overriding values
    var form = jQuery("#addToCart");
    var newData = {
        'dropdown-selection': dropdownSelection,
        a : asin ? asin : form.find("input[name=ASIN]").attr('value'),
        me: me ? me : form.find("input[name=merchantID]").attr('value'),
        olid: form.find("input[name=offerListingID]").attr("value"),
        wdg: 'book_display_on_website',
        dd: form.find("input[name=dd]").attr("value"),
        rps: form.find("input[name=rps]").attr("value"),
        quantity: jQuery("#quantity").attr("value"),
        horizonte: '1'
    };

    if(newData['quantity']){
        jQuery.extend(urlParams, newData);

        jQuery.ajax({
            url: '/gp/product/du/dcx/one-click-swatch-update.html',
            data: urlParams,
            dataType: 'html',
            success: function(ocHTML){
                window.BXPrimeSwatchFuncs.populateSwatches(ocHTML);
            },
            error: function(msg) {
                window.BXPrimeSwatchFuncs.populateSwatches('');
            }
        });

    }
}

window.BXPrimeSwatchFuncs.populateSwatches = function (content) {
    jQuery('#getItBy').html(content);

    window.BXPrimeSwatchFuncs.updateOneClickButton();

    window.BXPrimeSwatchFuncs.enableOneClickButton();
}

window.BXPrimeSwatchFuncs.getKeyPress = function (e, count) {
    var charCode;

    
    if (e) {
        charCode = e.keyCode || e.charCode;
    }

    
    if (charCode == 13) {
        window.BXPrimeSwatchFuncs.updateOneClickSwatchSelection(count);
        return false;
    }

    
    return (charCode == 9);
}

function isBXPrimeSwatchFuncsRegistered() {
	var isPrimeSwatchFuncsRegistered = false;
	AmazonUIPageJS.now("BXPrimeSwatchFuncs").execute(function (module) {
		isPrimeSwatchFuncsRegistered = !!module;
	});
	return isPrimeSwatchFuncsRegistered;
}

if (!isBXPrimeSwatchFuncsRegistered()) {
    P.register("BXPrimeSwatchFuncs", function () {
        return window.BXPrimeSwatchFuncs;
    });
}

P.when('jQuery', 'ready').execute(function(jQuery) {
    
    
        jQuery('#quantity').change(function() {
            window.BXPrimeSwatchFuncs.refreshSwatchesForQuantity();
        });

    
        
        
            jQuery("#getItBy").css("display","block");
        
    


    jQuery('#addToCart').submit(function() {
        jQuery('#getItBy .BBOneClickInput').each(function() {
            var ociInputs = jQuery(this);
            var id = ociInputs.attr('oci_key');
            var val = ociInputs.attr('oci_val');
            if (jQuery('#' + id).length > 0) {
                jQuery('#' + id).val(val);
            } else {
                jQuery('#addToCart').append('<input type="hidden" id="' + id + '" name="' + id + '" value="' + val + '" />');
            }
        });
    });

});
P.when("A", "BuyBox:disableAfterOneClick").execute(function(A, disableAfterOneClick) {
    disableAfterOneClick("#oneClickBuyButton");
});
</script>
